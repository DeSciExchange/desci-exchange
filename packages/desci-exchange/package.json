    "env": {
      "jest": true,
      "es2020": true,
      "browser": true,
      "node": true
    },
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "args": "all",
          "argsIgnorePattern": "_",
          "varsIgnorePattern": "_"
